{
  "name": "protractor-flake",
  "version": "0.1.0",
  "description": "A wrapper for protractor to automatically re-run failed specs for a specific number of attempts",
  "main": "dist/index.js",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/NickTomlin/protractor-flake.git"
  },
  "scripts": {
    "serve": "bin/www",
    "test": "curl localhost:3000 && webdriver-manager update && protractor test/support/protractor-config/protractor-sharded.conf.js",
    "test:dev": "mocha --watch",
    "build": "babel -d dist src",
    "prepublish": "npm run build"
  },
  "bin": "./bin/protractor-flake",
  "author": "Nick Tomlin",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.8.20",
    "chai": "^3.2.0",
    "cookie-parser": "^1.3.5",
    "express": "^4.13.1",
    "express-session": "^1.11.3",
    "mocha": "^2.2.5",
    "morgan": "^1.6.1",
    "mversion": "^1.10.0",
    "protractor": "^2.1.0",
    "proxyquire": "^1.6.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "minimist": "^1.1.2",
    "core-js": "^0.9.18"
  },
  "keywords": [
    "protractor",
    "flake",
    "retry"
  ]
}
